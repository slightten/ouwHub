_G.Color = Color3.fromRGB(66, 135, 245)
_G.Color2 = Color3.fromRGB(66, 135, 245)

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/slightten/ui-libs/main/funni"))()
local window = library:CreateWindow("ouwHub", Enum.KeyCode.RightControl)
local tab1 = window:CreateTab("Main")
local tab2 = window:CreateTab("Misc")
local miscsect = tab2:CreateSector("Misc", 'left')
miscsect:AddLabel("made by ouw#7809")
local plr = game.Players.LocalPlayer
local function tp(place)
    plr.Character.HumanoidRootPart.CFrame = place
end
miscsect:AddButton('click to join my discord',function()
    syn.request({
        Url = "http://127.0.0.1:6463/rpc?v=1",
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json",
            ["Origin"] = "https://discord.com"
        },
        Body = game:GetService("HttpService"):JSONEncode({
            cmd = "INVITE_BROWSER",
            args = {
                code = "8bmQrDT75B"
            },
            nonce = game:GetService("HttpService"):GenerateGUID(false)
        }),
     })
end)

local sect1 = tab1:CreateSector("Main", 'left')

local function getEnemy()
    local dist, thing = math.huge
    for i,v in pairs(workspace:GetChildren()) do
        if v:IsA"Model" and v:FindFirstChild("Bite") then
            local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Hitbox.Position).magnitude
            if mag < dist then 
                dist = mag 
                thing = v 
            end
        end
    end
    return thing
end

local function getTool()
    for i,v in next, game.Players.LocalPlayer.Character:GetChildren() do
        if v:IsA"Tool" then
            return v
        end
    end
end

sect1:AddButton("Get best spear", function()
    game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.SetItemData:InvokeServer('jadedcrystal',"Spear","Equipped")
end)

sect1:AddButton("Get best spear for gems", function()
    game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.SetItemData:InvokeServer('gemstone',"Spear","Equipped")
end)

local Toggle = sect1:AddToggle("Shark Farm", false, function(a)
    aa = a
    while aa do task.wait()
        task.spawn(function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getEnemy().Hitbox.CFrame * CFrame.new(0,20,0)
            game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.MonsterHit:FireServer(getEnemy(),tostring(getTool()),true)
            task.spawn(function()
                game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer("SellEverything")
            end)
        end)
    end
end)
Toggle:AddKeybind(CRAP,function()
end)

local Toggle = sect1:AddToggle("basically useless", false, function(b)
    bb = b
    while bb do task.wait()
        for i,v in next, game:GetService("Workspace").DroppedItems:GetChildren() do
            if v:IsA"Model" and v:FindFirstChild("Ring") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
                game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer("SellEverything")
            end
        end
    end
end)
Toggle:AddKeybind(CRAP,function()
end)
